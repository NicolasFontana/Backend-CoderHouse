<div class="d-flex justify-content-center mt-5">
  <h2 class="text-primary">Historial</h2>
</div>
<% if(productos.content.length > 0) { %>
<div>
  <table class="table table-dark table-striped w-50 mx-auto mt-5">
    <tr class="table-dark">
      <th class="table-dark"></th>
      <th class="table-dark">Producto</th>
      <th class="table-dark">Precio</th>
    </tr>
    <% productos["content"].map((producto) => { %>
      <tr class="table-dark">
        <td class="table-dark"><img src="<%= producto.thumbnail %> alt="" width="100"></td>
        <td class="table-dark align-middle"><%=producto.title %></td>
        <td class="table-dark align-middle">$<%=producto.price %></td>
      </tr>
    <% }) %>
  </table>
</div>
<% } else { %>
<div class="d-flex justify-content-center mt-5">
  <div class="alert alert-danger" role="alert">
    No hay productos
  </div>
</div>
<% } %>