<div class="w-50 mx-auto bg-light p-3 mt-5">
  <h2 class="d-flex justify-content-center text-primary">Centro de mensajes</h2>
  <form  action="/" method="post" id="myChat" class="d-flex justify-content-start my-4">
    <input type="email" id="id" class="form-control me-3" placeholder="Email..." required>
    <input type="text" id="firstName" class="form-control me-3" placeholder="Nombre..." required>
    <input type="text" id="lastName" class="form-control me-3" placeholder="Apellido..." required>
    <input type="number" id="age" class="form-control me-3" placeholder="Edad..." required>
    <input type="text" id="alias" class="form-control me-3" placeholder="Alias..." required>
    <input type="text" id="avatar" class="form-control me-3" placeholder="URL del avatar..." required>
    <input type="text" id="message" class="form-control me-3" placeholder="Mensaje..." required>
    <input type="submit" value="Enviar" class="btn btn-primary">
  </form>
  <hr>
  <div id="container-messages"></div>
</div>
